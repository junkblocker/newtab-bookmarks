{
  "manifest_version": 2,
  "name": "newtab-bookmarks",
  "version": "1.0.1",
  "author": "Tom",
  "chrome_url_overrides" : {
    "newtab": "index.html"
  },
  "description":"New tab replacement",
  "permissions": [
    "bookmarks",
    "storage"
  ],
  "options_ui": {
    "page": "settings.html",
    "open_in_tab": true,
    "browser_style": true
  }
}
